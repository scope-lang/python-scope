var cpow=(obj,p)->{
var angle=Math.atan2(obj.i,obj.r)*p;
var len=Math.pow(Math.pow(obj.i*obj.i+obj.r*obj.r,0.5),p);
return {r:Math.cos(angle)*len,i:Math.sin(angle)*len};
};
var cadd=(a,b)->{
return {r:a.r+b.r,i:a.i+b.i};
};
var mandelbrot=(x,y)->{
var c={r:x,i:y};
var z={r:x+0,i:y+0};
var m=0;
for(var n=0;n<10;n++){
  z=cadd(cpow(z,2),c);
  if(Math.pow(z.i*z.i+z.r*z.r,0.5)<2) {
    m=n+0;
  }
}
return m>6;
};


var pictureSize=40;
var xScale=2/3;
for(var yy=0;yy<pictureSize;yy++){
var row="";
  for(var xx=0;xx<(pictureSize/xScale);xx++){
    var re=(4*xx/pictureSize)*xScale-2;
    var im=4*yy/pictureSize-2;
    //console.log(re,im);
    row=row+(mandelbrot(re,im)?" ":"0");
  }
  console.log(row);
};
